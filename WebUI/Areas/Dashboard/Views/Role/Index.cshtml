@model List<GetRolesDTO>


<div asp-validation-summary="All" class="text text-danger"></div>




<table id="example" class="display table table-striped" style="width:100%">
    <thead>
        <tr>

            <th>#</th>
            <th>Rol Adı</th>
         
           
           

        </tr>

    </thead>
    <tbody id="parent">
        @{
            int counter = 0;
        }
        @foreach (var role in Model)
        {
            counter++;

      <tr>
            <td>@counter</td>
            <td>@role.RoleName</td>
      
              
           
        </tr>
        }
    </tbody>

    <tfoot>
        <tr>

            <th>#</th>
            <th>Rol Adı</th>
       



        </tr>

    </tfoot>
</table>

<script>
    let table = new DataTable('#example', {
        responsive: true
    });
   
</script>